{
  "expo": {
    "name": "Self Blog",
    "slug": "rn-blog-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIndentifier": "com.vladyslavdev.self-blog-react-native-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.vladyslavdev.self_blog_react_native_app",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    }
  }
}
